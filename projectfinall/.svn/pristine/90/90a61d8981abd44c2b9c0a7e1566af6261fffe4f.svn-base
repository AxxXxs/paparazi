/*首页底部*/
#btn{
    background:white;
    width:100%;
    position:fixed;
    bottom:0;
    padding-top: 0.2rem;;
    border-top: 0.03rem solid rgb(187,187,187);
    display:flex;
    justify-content:space-around;
    color:black;
    font-size: 0.26rem;
}
#btn img{
    width: 55%;
}
#btn div{
    text-align:center;
}
/*首页头部*/
.nav1{
    margin:20px 0;
    display:flex;
    justify-content:space-around;
    font-size:0.4rem;
    color:rgb(212,47,47);
    text-align:center;
}
.navText{
    box-sizing:border-box;
    width:85%;
}
.navImg{
    width:15%;
    margin-top:-2px;
}
/*图片轮播*/
.imgBox{
    position:relative;
    width:100%;
    overflow:hidden;
    background:rgb(226,226,226);
    padding:12px 0;
}
.homeLb{
    width:400%;
    overflow:hidden;
    transition:all 2s;
}
.homeLb img{
    float:left;
    width:25%;
}

/*轮播小按钮*/
.btnLb{
    width:100%;
    overflow:hidden;
    position:absolute;
    margin-top:-40px;
    left:30%;
}
.btnLb li{
    border:1px solid white;
    padding:8px;
    border-radius:50%;
    background:rgba(150,151,161,0.8);
    float:left;
    margin:0 4%;
    z-index:999;
}
.btnLb li:nth-child(1){
    background:red;
}


/*我的礼包*/
.gift{
    margin:2% 2%;
    overflow: hidden;
}
.gift1{

    text-align:center;
    font-size:0.35rem;
    color:white;
    background:rgb(244,98,89);
    width:50%;
    border-radius:20px;
    padding:1% 0 1% 5%;
    position:relative;

}
.gCil{
    width:0.6rem;
    height:0.6rem;
    background:white;
    border-radius:50%;
    position:absolute;
    left:1%;
    top:0.5%;
}
.gCil img{
    width:65%;
}
.g1{
    width:20%;
    float:left;
    margin:20px  2.5% 10px 2.5%;
    text-align:center;
    overflow: hidden;
}
.g1 img{
    width:100%;
}
.g1 h1{
    font-size:0.3rem;
}
.g1 p{
    margin-top: 1em;
    font-size: 0.28rem;
}
.g1 div{
    color:rgb(171,171,171);
}
.g1 span{
    color:red;
}
.llb{
    width:100%;
    overflow:hidden;
    padding:10px 0;
}
.llb1{
    border-bottom:2px solid #dcdcdc;
}
.llb div{
   float:left;
   line-height:30px;
}
.gBox{
    overflow:hidden;
}
.limg {
    width:20%;
    margin-right:10px;
}
.limg img{
    width:100%;
}
.llb p{
    color:gray;
    opacity:0.8;
}
.giftFoot{
    text-align:right;
    color:red;
}
.giftDate{
    font-size: 0.1rem;
    margin-top:3px;
    text-align:right;
}

.giftNr h6{
    font-size: 0.3rem;
}
.giftNr p{
    font-size: 0.28rem;
}

.game{
    padding-top:15px;
    background:rgb(226,226,226);
    margin-bottom:15px;

}
.game1{
    background:white;
    overflow:hidden;
}
.ptiImg{
    position:relative;
    top:15px;
    font-size:0.4rem;
}
.timg{
    width:18%;
}
.lgImg{
    width:100%;
}
.ptiImg1{
    margin:0 5px;
}
.pxImg,.pxImg1{
    padding:20px 0;
    font-size:0.3rem;
    color:#555;
    margin:0 10px;
}
.pxImg{
    border-bottom:2px solid #dcdcdc;
}
.xImg,.xImg1{
   width:20%;
}
.pmore{
    text-align:center;
    margin:0 10px;
}
.more{
    width:100%;
    height:50px;
    background:rgb(254,119,111);
    color:white;
    font-size:0.35rem;
    border:none;
}
.video{
    width:45%;
    float:left;
    margin:0px 2.5% 10px;
    color:rgb(139,139,139);
    font-size:0.27rem;
}
.video img{
    width:100%;
    margin-bottom: 0.15rem;
}
.vImg,.vImg1{
    color:#555;
    margin:0 20px;
    font-size:0.4rem;
}
.vImg{
    font-size:0.4rem;
    padding:20px 0 10px;
}
.gam{
    margin-bottom:100px;
}
.yh{
    display:inline-block;
    width: 0.5rem;
    height: 0.5rem;
    border-radius:50%;
    background: #ff6158;
}
.yh img{
    width:50%;
    margin-left:30%;
    margin-top:-0.15rem;
}

/*新手攻略*/
.strategyContent li{
    padding: 0.23rem;
    position: relative;
    border-bottom: 1px solid rgb(221,221,221);
}
.strategyContent li img{
    width:38%;
}
.strategyContent li p{
    width: 58%;
    font-size:0.28rem ;
    position: absolute;
    top:0.15rem;
    right: 0.1rem;
    padding: 0.1rem  0 0.1rem 0.1rem;
}
.strategyContent li em{
    font-size: 0.2rem;
    color:rgb(221,221,221);
    padding: 0.2rem 0.2rem;
    position: absolute;
    bottom:0.1rem;
}
.strategyContent li:last-child{
    border: none;
}

/*左弹窗*/
.add{
    width: 85%;
    background: #fafafa;
    position: absolute;
    z-index: 55;
    top:0;
    padding-bottom: 5.9rem;
    box-shadow: 30px 0 200px black;
    display: none;
}
.head-portrait{
    margin-top: 0.6rem;
}
.head-portrait img{
    width: 25%;
}
.load-btn{
    margin-top: 0.4rem;
}
.load-btn button{
    width: 2rem;
    font-size: 0.3rem;
}
.text{
    background: white;
    margin-top: 0.5rem;
}
.text p{
    margin-top: 0.5rem;
    font-size: 0.44rem;
}
.text a{
    display: block;
    margin-top: 0.6rem;
    margin-bottom: 0.5rem;
}
.text img{
    width: 11%;
}
.text span{
    margin-left: 0.2rem;
    font-size: 0.35rem;
    vertical-align: middle;
}
.fot{
    margin-top: 0.4rem;
    margin-left: 0.4rem;
}
.fot img{
    width: 7%;
    margin-right: 0.5em;
}
.fot span{
    font-size: 0.32rem;
    color: rgb(196,196,200);
    vertical-align: middle;
}


































