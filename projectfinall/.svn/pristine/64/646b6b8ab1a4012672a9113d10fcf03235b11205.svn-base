.my{
    margin:0.3rem;
}
.my img{
    width: 8%;
}
.myWall{
    background-image: url("../img/my-1.png");
    background-size: 100%;
    overflow: hidden;
}
.myContent{
    width: 90%;
    margin: auto;
    background-color: rgba(255,255,255,0.9);
    margin-top: 3.2rem;
    position: relative;
}
.myHead{
    width: 25%;
    position: absolute;
    top: -1rem;
    left: 2.5rem;
}
.myHead img{
    width: 100%;
}
.myLoad{
    padding-top: 0.8rem;
}
.myLoad p{
    font-size: 0.3rem;
}
.myLoad button{
    font-size: 0.3rem;
    width: 1.4rem;
}
.sign{
    overflow: hidden;
    padding: 0.3rem;
    border-bottom: solid 1px rgb(229,229,229);
}
.myLeft{
    margin-left: 0.5rem;
}
.myLeft span{
    font-size: 0.28rem;
}
.myLeft u{
    color: orange;
}
.myLeft a{
    margin-left: 0.1rem;
    font-size: 0.22rem;
    text-decoration: underline;
    color: rgb(242,99,93);
}
.myRight{
    margin-right: 0.2rem;
}
.myRight a{
    font-size: 0.28rem;
    color: rgb(242,99,93);
}
.myRight a img{
    width: 26%;
    margin-right: 0.1rem;
    vertical-align: top;
}
.myRight span{
    text-decoration: underline;
}
.ask{
    padding-bottom: 1.5rem;
}
.ask li{
    border-bottom: solid 1px rgb(229,229,229);
    padding: 0.05rem;
}
.ask li a{
    overflow: hidden;
    vertical-align: middle;
    font-size: 0.28rem;
}
.askImg1{
    width: 5.5%;
    margin: 0.2rem 0.2rem 0.2rem 0.3rem;
}
.askImg2{
    width: 2%;
    margin-top: 0.25rem;
    margin-right: 0.3rem;
}


/*我的收藏部分*/
.strContent{
    background-color:rgb(251,251,251);
}
.strContent1{
    display:flex ;
    justify-content: space-around;
    background-color:white;
}
.strContent1 li{
    text-align: center;
    padding: 0.15rem 0;
    position: relative;
    font-size: 0.34rem;
}
.strContent1 li div{
    position: absolute;
    width: 1.3rem;
    height: 0.03rem;
    background-color: red;
    margin-left: -0.3rem;
    bottom: -0.07rem;
    z-index: 99;
}

.strContent2 li{
    position: relative;
    height: 1.4rem;
    border-bottom: 1px solid rgb(177,177,177);
    box-sizing: border-box;
}
.strContent2 li p{
    padding-top: 0.2rem ;
    font-size: 0.26rem;
    color: black;
    text-align: center;
}
.strContent2 li em{
    color:rgb(177,177,177);
    font-size: 0.2rem;
    position: absolute;
    bottom:0.2rem;
    right: 0.5rem;
}
.strContent2 li div{
    position: absolute;
    top: 0px;
    left: 0.6rem;
    width: 0.07rem;
    height: 1.4rem;
    background-color: #727272;
    z-index: 99;
}
.strContent2 li div img{
    position: absolute;
    width: 0.6rem;
    margin-top: -0.3rem;
    margin-left: -0.3rem;
    top:50%;
    left: 50%;
}
.strContent2 li a{
    position: absolute;
    top:0;
    right: -1.5rem;
    display: block;
    width: 1.5rem;
    height: 1.4rem;
    background: rgb(127,89,127);
    color: white;
    line-height: 1.4rem;
    text-align: center;
    font-size: 0.3rem;
}

/*关于我们*/
.about{
    position: relative;
    width: 80%;
    position: fixed;
    top:50%;
    left:9.5%;
    background-color: rgba(250,250,250,0.9);
    padding-bottom: 1rem;
    display: none;
}
.about p:nth-child(1){
    background-color: rgba(241,241,241,.8);
    font-size: 0.32rem;
    padding: 0.2rem;
    box-sizing: border-box;
}
.about p:nth-child(2),.about p:nth-child(3){
    font-size: 0.25rem;
    padding: 0 0.5rem 0 0.5rem;
    line-height: 0.4rem;
    color: #555555;
}
.about p:nth-child(4) {
    position: absolute;
    top: 0.2rem;
    right: 0.4rem;
    color: #ababab;
    font-weight: 800;
}

















